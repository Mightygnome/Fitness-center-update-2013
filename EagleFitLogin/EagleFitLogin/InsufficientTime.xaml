<Window x:Name="InsufficientTime1" x:Class="EagleFitLogin.InsufficientTime"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Insufficient Time To Earn Next Visit" Height="257.303" Width="419.101">
    <Grid>
        <Label x:Name="lbl_close" Content="You are close to earning a visit" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="37,20,0,0" Width="350" Height="42" FontWeight="Bold" FontSize="22"/>
        <Label x:Name="lbl_continue" Content="Would you like to continue your workout?" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="20,96,0,0" Width="367" FontSize="18" FontWeight="Bold"/>
        <Button x:Name="btn_yes" Content="YES, cancel logout" HorizontalAlignment="Left" VerticalAlignment="Top" Width="162" Margin="20,135,0,0" Height="82" FontSize="16" FontWeight="Bold" Click="btn_yes_Click"/>
        <Button x:Name="btn_no" Content="NO, log me out now" HorizontalAlignment="Left" VerticalAlignment="Top" Width="162" Margin="225,135,0,0" Height="82" FontWeight="Bold" FontSize="16" Click="btn_no_Click"/>

    </Grid>
</Window>
